{
  "ignorePatterns": [
    {
      "pattern": "^https://github\\.com/.+/blob/.+#L\\d+"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^https://inkwell-writing\\.vercel\\.app"
    },
    {
      "pattern": "^https://dashboard\\.supabase\\.com"
    },
    {
      "pattern": "^https://github\\.com/oklahomahail/Inkwell2/discussions"
    },
    {
      "pattern": "^\\.\\./supabase/"
    },
    {
      "pattern": "^\\.\\./\\.github/"
    },
    {
      "pattern": "\\.test\\.ts$"
    },
    {
      "pattern": "\\.test\\.ts#"
    },
    {
      "pattern": "\\.sql$"
    },
    {
      "pattern": "\\.sql#L\\d+"
    },
    {
      "pattern": "supabase/migrations/.+\\.sql"
    },
    {
      "pattern": "^\\.\\./SECURITY"
    },
    {
      "pattern": "^/SECURITY"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206, 403]
}
