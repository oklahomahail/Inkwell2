{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [{ "source": "/:path*", "destination": "/" }],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    },
    {
      "source": "/workbox-:hash.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ]
}
