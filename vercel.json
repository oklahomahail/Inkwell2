{
  "version": 2,
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://writewithinkwell.com/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "inkwell.leadwithnexus.com"
        }
      ]
    },
    {
      "source": "/:path*",
      "destination": "https://writewithinkwell.com/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.writewithinkwell.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!_next/|assets/|brand/|icons/|logo/|.*\\..*).*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/brand/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "functions": {
    "api/export/pdf.ts": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/telemetry.ts": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}
