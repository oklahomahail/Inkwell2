{
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!_next/|assets/|brand/|icons/|logo/|.*\\..*).*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/assets/brand/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "functions": {
    "api/export/pdf.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
