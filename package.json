{
  "name": "inkwell",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:css": "stylelint '**/*.css'",
    "lint:css:fix": "stylelint '**/*.css' --fix",
    "vercel:test": "npm run build && npm run preview",
    "format": "prettier --write .",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "tailwind:replace": "node scripts/tailwind-text-replacer.mjs",
    "lint:ci": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=314",
    "repo:audit": "pnpm -s knip && pnpm -s unimported && pnpm -s depcheck && pnpm -s madge:orphans && pnpm -s madge:cycles",
    "knip": "knip",
    "unimported": "unimported --flow -e src/main.tsx",
    "depcheck": "depcheck --skip-missing=true",
    "madge:orphans": "madge --extensions ts,tsx --ts-config tsconfig.json --orphans src",
    "madge:cycles": "madge --extensions ts,tsx --ts-config tsconfig.json --circular src",
    "audit:knip": "knip --dependencies --files --reporter compact --no-exit-code",
    "audit:depcheck": "depcheck --skip-missing=true || true",
    "audit:orphans": "madge --extensions ts,tsx --ts-config tsconfig.json --orphans src/main.tsx || true",
    "audit:cycles": "madge --extensions ts,tsx --ts-config tsconfig.json --circular src/main.tsx || true"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tiptap/core": "^3.2.0",
    "@tiptap/extension-character-count": "^3.1.0",
    "@tiptap/extension-history": "^3.2.0",
    "@tiptap/extension-placeholder": "^3.1.0",
    "@tiptap/extension-typography": "^3.1.0",
    "@tiptap/pm": "^3.1.0",
    "@tiptap/react": "^3.2.2",
    "@tiptap/starter-kit": "^3.1.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.541.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.8.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.15",
    "depcheck": "^1.4.7",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "husky": "^9.0.0",
    "knip": "^5.63.0",
    "lint-staged": "^15.2.0",
    "madge": "^8.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.6.2",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.4",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "unimported": "^1.31.1",
    "vite": "^7.0.6"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-is": "^19.1.1"
    }
  },
  "packageManager": "pnpm@9.12.0"
}
