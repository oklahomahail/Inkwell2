{
  "name": "inkwell",
  "version": "1.2.1",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22.x",
    "pnpm": ">=9"
  },
  "scripts": {
    "changeset": "changeset",
    "release": "changeset publish",
    "tree": "npx tree -L 2 -I 'node_modules|dist|build|coverage|.vercel|.DS_Store'",
    "tree:update": "node scripts/insert-tree.js",
    "readme:verify": "pnpm tree:update && git diff --exit-code README.md || (echo 'README.md tree out of date. Run pnpm tree:update locally.' && exit 1)",
    "hygiene": "git grep -nE 'TODO\\(>30d\\)' -- ':!node_modules' && exit 1 || exit 0",
    "prettier:check": "prettier --check .",
    "check:env": "node scripts/check-env-vars.mjs",
    "build:icons": "magick public/brand/inkwell-icon.svg -resize 32x32 public/brand/inkwell-icon-32.png && magick public/brand/inkwell-icon.svg -resize 180x180 public/brand/inkwell-icon-180.png && magick public/brand/inkwell-icon.svg -resize 192x192 public/brand/inkwell-icon-192.png && magick public/brand/inkwell-icon.svg -resize 512x512 public/brand/inkwell-icon-512.png && magick public/brand/inkwell-lockup-light.svg -resize 1200x630 public/brand/inkwell-og-1200x630.png",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:test": "tsc --noEmit -p tsconfig.test.json",
    "build:strict": "VITE_ENABLE_PWA=false tsc --noEmit && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:hooks": "eslint \"src/**/*.{ts,tsx}\" --config eslint.config.hooks.js",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:ci": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=50",
    "lint:strict": "eslint . --max-warnings=0",
    "lint:json": "eslint 'src/**/*.{ts,tsx}' -f json -o eslint-report.json",
    "fix:logs": "jscodeshift -t scripts/codemods/console-to-devlog.js 'src/**/*.{ts,tsx,js,jsx}'",
    "fix:logs:regex": "node scripts/replace-console-logs.mjs",
    "fix:unused": "node scripts/prefix-unused-from-eslint.mjs eslint-report.json",
    "fix:auto": "pnpm fix:logs:regex && pnpm lint:json && pnpm fix:unused && eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "format": "prettier --write .",
    "prepare": "git rev-parse --is-inside-work-tree >/dev/null 2>&1 && husky install || echo 'skip husky on CI'",
    "tailwind:replace": "node scripts/tailwind-text-replacer.mjs",
    "clean": "rm -rf node_modules pnpm-lock.yaml node_modules/.cache .turbo .vite dist",
    "vercel:test": "npm run build && npm run preview",
    "supabase:setup": "bash scripts/setup-supabase.sh",
    "supabase:migrate": "bash supabase/apply-migration.sh",
    "supabase:push": "npx supabase db push",
    "supabase:types": "npx supabase gen types typescript --linked > src/types/supabase.ts",
    "supabase:health": "open http://localhost:5173/health/supabase",
    "test": "vitest run --reporter=verbose",
    "test:typecheck": "tsc --noEmit -p tsconfig.test.json && vitest run",
    "test:ci": "pnpm typecheck:test && vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:diff": "vitest run --coverage && node scripts/coverage-diff.mjs",
    "test:baseline": "tsx src/test/runBaseline.ts",
    "test:perf": "vitest run tests/performance/baseline.test.ts",
    "test:perf:report": "pnpm test:perf && node scripts/performance-report.mjs",
    "coverage:report": "node scripts/coverage-diff.mjs",
    "coverage:save-baseline": "cp coverage/coverage-summary.json coverage-baseline.json",
    "bench": "vitest bench",
    "audit:baseline": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project .audit/tsconfig.json .audit/test-audit.ts baseline",
    "audit:probe": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project .audit/tsconfig.json .audit/test-audit.ts probe",
    "audit:probe:files": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project .audit/tsconfig.json .audit/test-audit.ts probe --skipFiles",
    "audit:clean": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project .audit/tsconfig.json .audit/test-audit.ts clean",
    "audit:knip": "knip --dependencies --files --reporter symbols --no-exit-code --tsConfig tsconfig.json",
    "audit:depcheck": "depcheck --skip-missing=true || true",
    "audit:orphans": "madge --extensions ts,tsx --ts-config tsconfig.json --orphans src | grep -Ev '^(src/main\\.tsx|src/vite-env\\.d\\.ts)$' || true",
    "audit:cycles": "madge --extensions ts,tsx --ts-config tsconfig.json --circular src || true",
    "verify-tour-anchors": "node scripts/verify-tour-anchors.cjs",
    "verify-tour-anchors:verbose": "node scripts/verify-tour-anchors.cjs --verbose",
    "test:smoke:tour": "playwright test tests/smoke/tour-smoke.spec.ts",
    "test:anchors": "vitest run src/tour/__tests__/anchors.test.ts",
    "predeploy:tour": "./scripts/tour-predeploy-check.sh",
    "repo:audit": "pnpm -s audit:knip && pnpm -s audit:depcheck && pnpm -s audit:orphans && pnpm -s audit:cycles",
    "ci": "pnpm -s typecheck && pnpm -s lint && pnpm -s test:run && pnpm -s ci:audit",
    "ci:audit": "mkdir -p reports && pnpm -s audit:knip | tee reports/knip.txt && pnpm -s audit:depcheck | tee reports/depcheck.txt && pnpm -s audit:orphans | tee reports/madge-orphans.txt && pnpm -s audit:cycles | tee reports/madge-cycles.txt",
    "lint:relaxed": "eslint \"src/**/*.{ts,tsx}\" --fix --cache --cache-location node_modules/.cache/eslint",
    "pretypecheck": "npm run lint:relaxed",
    "check": "pnpm test && pnpm lint",
    "check:corruption": "./scripts/check-corruption.sh",
    "audit": "bash scripts/run-audit.sh",
    "audit:quick": "bash scripts/run-audit.sh --quick",
    "audit:ci": "bash scripts/run-audit.sh --ci",
    "audit:ship-blockers": "bash scripts/ci/check-ship-blockers.sh"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "eslint --config eslint.config.hooks.js",
      "prettier --write",
      "bash -c 'tsc --noEmit'"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@sentry/react": "^10.19.0",
    "@supabase/supabase-js": "^2.75.1",
    "@tanstack/react-virtual": "^3.13.12",
    "@tiptap/core": "^3.2.0",
    "@tiptap/extension-character-count": "^3.1.0",
    "@tiptap/extension-history": "^3.2.0",
    "@tiptap/extension-placeholder": "^3.1.0",
    "@tiptap/extension-typography": "^3.1.0",
    "@tiptap/pm": "^3.1.0",
    "@tiptap/react": "^3.2.2",
    "@tiptap/starter-kit": "^3.1.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.24",
    "idb": "^8.0.3",
    "jose": "^6.1.1",
    "jszip": "^3.10.1",
    "libsodium-wrappers-sumo": "^0.7.15",
    "lucide-react": "^0.552.0",
    "nanoid": "^5.1.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "uuid": "^13.0.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/history": "^5.0.0",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vercel/node": "^5.3.24",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.2.0",
    "fake-indexeddb": "^6.2.4",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "history": "^5.3.0",
    "husky": "^8.0.3",
    "jscodeshift": "^17.3.0",
    "jsdom": "^26.1.0",
    "knip": "^5.66.2",
    "lint-staged": "^15.5.2",
    "madge": "^8.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^39.0.1",
    "supabase": "^2.54.11",
    "tailwindcss": "^3.4.4",
    "tree-cli": "^0.6.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.10",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-is": "^19.1.1"
    }
  },
  "packageManager": "pnpm@9.12.0"
}
